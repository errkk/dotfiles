[user]
    email = eric@madebymany.co.uk
    name = Eric George
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[merge]
    log = true
[core]
    editor = 'nvim'
	excludesfile = /Users/eric/.gitignore_global
[push]
    default = simple
[alias]
	g = grep --break --heading --line-number
	up = pull --rebase --autostash
